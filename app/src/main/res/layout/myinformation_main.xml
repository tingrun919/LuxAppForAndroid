<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/loginColor"
    >

    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/btn_login"
        android:id="@+id/view3"
        android:layout_below="@+id/my_qm"
        android:layout_alignParentStart="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/btn_login"
        android:id="@+id/view2"
        android:layout_below="@+id/my_nc"
        android:layout_alignParentStart="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/btn_login"
        android:id="@+id/view1"
        android:layout_below="@+id/my_tx"
        android:layout_alignParentStart="true"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/btn_login"
        android:id="@+id/view4"
        android:layout_marginTop="72dp"
        android:layout_below="@+id/view2"
        android:layout_alignParentStart="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/btn_login"
        android:id="@+id/view5"
        android:layout_marginTop="57dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/ud_ps"
        android:id="@+id/btn_udmyi"
        android:textColor="@color/btn_login"
        android:textSize="22dp"
        android:background="@drawable/btn_register"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="95dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingEnd="15dp"
        android:paddingRight="15dp"
        android:textSize="18dp"
        android:text="@string/pic"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        android:textColor="#B0B0B0"
        android:background="@color/loginColor"
        android:id="@+id/my_tx"
        android:layout_below="@+id/view5"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/my_pic"
        android:layout_alignParentEnd="true"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingEnd="15dp"
        android:paddingRight="15dp"
        android:paddingLeft="20dp"
        android:hint="昵称"
        android:textColorHint="#B0B0B0"
        android:background="@color/loginColor"
        android:id="@+id/my_nc"
        android:layout_below="@+id/view1"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingEnd="15dp"
        android:paddingRight="15dp"
        android:paddingLeft="20dp"
        android:background="@color/loginColor"
        android:id="@+id/my_qm"
        android:layout_below="@+id/view4"
        android:layout_alignParentStart="true"/>



    <order.smzs.com.companyorder.image.SmartImageView
        android:layout_width="70dp"
        android:id="@+id/my_pic"
        android:layout_height="70dp"
        android:layout_margin="10dp"
        android:layout_below="@+id/view5"
        android:layout_alignParentEnd="true"/>

</RelativeLayout>
